{
  "name": "pully-svc",
  "version": "0.3.0",
  "description": "Youtube channel and playlist auto-downloader!",
  "main": "index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "pully-svc": "./dist/bin/pully-svc.js"
  },
  "scripts": {
    "clean": "rimraf ./dist/** ./coverage",
    "build": "tsc",
    "watch": "concurrently npm:watch:*",
    "watch:build": "tsc --watch",
    "watch:test": "ava --watch",
    "test": "tsc && nyc --color -a ava -v",
    "debug": "tsc && node ./dist/debug",
    "trial": "tsc && cd test && pully-svc",
    "example": "tsc && node ./dist/example",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "preversion": "npm test",
    "version": "git add -A",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JimmyBoh/pully-server.git"
  },
  "author": "Jim Buck <jim@jimmyboh.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JimmyBoh/pully-server/issues"
  },
  "homepage": "https://github.com/JimmyBoh/pully-server#readme",
  "devDependencies": {
    "@types/debug": "4.1.0",
    "@types/got": "^9.4.0",
    "@types/ink": "^0.5.1",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "10.12.23",
    "@types/strip-ansi": "^3.0.0",
    "@types/yargs": "^12.0.8",
    "ava": "^1.2.1",
    "coveralls": "^3.0.2",
    "nyc": "^13.2.0",
    "rimraf": "^2.6.3",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "flexel": "^0.7.3",
    "got": "^12.5.3",
    "ink": "^0.5.1",
    "ink-divider": "^1.1.1",
    "ink-progress-bar": "^2.1.1",
    "ink-tab": "^1.3.0",
    "ink-table": "^1.0.3",
    "mkdirp": "^0.5.1",
    "multimatch": "^3.0.0",
    "parse-duration": "^0.1.1",
    "pully": "^2.7.0",
    "pully-core": "^0.7.4",
    "scany": "^1.3.1",
    "skedgy": "^0.8.0",
    "yargs": "^12.0.5"
  },
  "ava": {
    "files": [
      "./dist/**/*.spec.js"
    ],
    "sources": [
      "./dist/**/*.js"
    ]
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "exclude": [
      "dist/**/*.spec.js"
    ]
  }
}
